{
  "private": false,
  "version": "0.1.0",
  "name": "@kiwicom/iam",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc ava",
    "test-ci": "npm run test && tsc --noEmit",
    "build": "rm -rf dist && tsc --declaration",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "graphql": "^14.2.1",
