    "node-fetch": "^2.5.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/node-fetch": "^2.3.3",
    "ava": "^2.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0"
  },
  "ava": {
    "compileEnhancements": false,
    "cache": true,
    "concurrency": 5,
    "extensions": [
      "ts"
