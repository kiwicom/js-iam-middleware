{
  "private": false,
  "version": "0.1.0",
  "name": "@kiwicom/iam",
  "repository": "kiwicom/js-iam-middleware",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc ava",
    "lint": "eslint . --ext ts",
    "test-ci": "npm run test && npm run lint && tsc --noEmit",
