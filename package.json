    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.2",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "mamushi": "^1.1.5",
    "node-fetch": "^2.6.0",
    "opn": "^6.0.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "@types/node-fetch": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "ava": "^2.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0"
  },
  "ava": {
    "compileEnhancements": false,
    "cache": true,
    "concurrency": 5,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/**/*.test.ts"
    ]
  }
}
